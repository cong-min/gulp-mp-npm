{
  "name": "gulp-mp-npm",
  "version": "1.9.7",
  "description": "用以微信小程序提取 npm 依赖包的 gulp 插件",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "lib",
    "index.js"
  ],
  "scripts": {
    "test": "jest",
    "test:cover": "jest --collect-coverage && codecov -t"
