  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "fancy-log": "^1.3.3",
    "import-regex": "^1.1.0",
    "lead": "^3.0.0",
    "load-json-file": "^6.2.0",
    "lodash": "^4.17.20",
    "precinct": "^8.1.0",
    "pumpify": "^2.0.1",
    "read-package-tree": "^5.3.1",
    "resolve": "^1.20.0",
    "through2": "^4.0.2",
    "tree-to-list": "^2.0.0",
    "vinyl-fs": "^3.0.3"
  },
  "devDependencies": {
    "@miniprogram-component-plus/sticky": "=1.0.1",
    "@sindresorhus/slugify": "=2.0.0",
    "@sindresorhus/transliterate": "=1.2.0",
    "codecov": "^3.8.2",
    "del": "^6.0.0",
    "eslint": "^7.26.0",
    "eslint-config-soda-works": "^1.2.0",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-less": "=4.0.1",
    "gulp-rename": "=2.0.0",
    "gulp-sourcemaps": "=2.6.5",
    "gulp-typescript": "=6.0.0-alpha.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jsonfile": "=6.0.1",
    "lint-staged": "^11.0.0",
    "lodash": "=4.17.20",
    "miniprogram-api-promise": "=1.0.4",
    "miniprogram-api-typings": "=3.1.0",
    "mitt": "=2.1.0",
    "mkdirp": "^1.0.4",
    "typescript": "=4.0.3",
    "weui-miniprogram": "=1.0.5",
    "weui-wxss": "=2.4.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "index.js",
      "lib/**"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
